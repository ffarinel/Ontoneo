        <oboInOwl:hasDbXref rdf:datatype="&xsd;string">MeSH:D015615</oboInOwl:hasDbXref>
        <oboInOwl:hasOBONamespace rdf:datatype="&xsd;string">human_phenotype</oboInOwl:hasOBONamespace>
